{
  "manifest_version": 3,
  "name": "Pinora",
  "version": "1.1.0",
  "description": "A browser extension that displays bookmarks on new tab page",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
